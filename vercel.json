{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      },
      {
        "src": "/socket.io/",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/index.html"
      }
    ],
    "outputDirectory": "client/build"
  }
  